<h1 style="color:white;">Home</h1>

<nav>
  <ul class="navbar">
    <li class="navbar-item"><%= link_to "Home", home_path %></li>
    <li class="navbar-item"><%= link_to "Market", market_path %></li>
    <li class="navbar-item"><%= link_to "Trade/Portfolio", trade_path %></li>
    <li class="navbar-item"><%= link_to "Transaction"%></li>
    <li class="navbar-item"><%= link_to "Signup", signup_path %></li>
  </ul>
</nav>

<div class="user-wrapper">
<% @users.each do |user| %>
    <div class="user-profile">
      <p class="user-details"><%= user.username %></p>
      <p class="user-details"><%= user.email %></p>
      <p class="user-details"><%= user.password %></p>
    </div>
<% end %>
</div>